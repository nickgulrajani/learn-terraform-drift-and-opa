provider "aws" {
  region = var.aws_region
  shared_credentials_file = "$HOME/.aws/credentials"
  
}
terraform {
  cloud {
    organization = "cdw-dvs"
    region = var.aws_region
    shared_credentials_file = "$HOME/.aws/credentials"
    workspaces {
      name = "test-opa-policies"
    }
  }
}

module "network" {
  source = "./modules/network"

  bastion_instance_type = var.bastion_instance_type
}
